{
    "name": "sourcefabric/payum-mbe4",
    "type": "library",
    "description": "The Payum extension. It provides Mbe4 payment integration.",
    "keywords": ["payment", "payum", "mbe4", "phone", "bill"],
    "homepage": "https://sourcefabric.org",
    "license": "GPLv3",
    "authors": [
        {
            "name": "Rafał Muszyński",
            "email": "rafal.muszynski@sourcefabric.org"
        }
    ],
    "require": {
        "php": "^7.1",
        "payum/core": "^1.3",
        "phpspec/phpspec": "^4.0"
    },
    "require-dev": {
        "payum/core": "^1.3@dev",
        "php-http/guzzle6-adapter": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Sourcefabric\\Payum\\Mbe4\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": { "Sourcefabric\\Payum\\Mbe4\\spec\\": "spec/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
